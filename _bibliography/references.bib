  Pii                      = {S0165-0270(12)00276-2},
  Pmid                     = {22841940},
  Timestamp                = {2015.06.06},
  Url                      = {http://dx.doi.org/10.1016/j.jneumeth.2012.07.012}
}

@Article{Yucesoy2008c,
  Title                    = {{SCIWORA in MRI era.}},
