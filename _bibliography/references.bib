@Article{Kim2010,
  Title                    = {{Which lumbar interbody fusion technique is better in terms of level for the treatment of unstable isthmic spondylolisthesis?}},
  Author                   = {Kim, Jin-Sung and Lee, Kil-Yong and Lee, Sang-Ho and Lee, Ho-Yeon},
