@Article{Medina2012,
  Title                    = {{Stochastic facilitation of artificial tactile sensation in primates.}},
  Author                   = {Medina, Leonel E and Lebedev, Mikhail A and O'Doherty, Joseph E and Nicolelis, Miguel A L},
