@Article{Kim2003a,
  Title                    = {Method for unsupervised classification of multiunit neural signal recording under low signal-to-noise ratio.},
  Author                   = {Kim, Kyung Hwan and Kim, Sung June},
  Journal                  = {IEEE Trans Biomed Eng},
