  Keywords                 = {Choice Behavior,Choice Behavior: physiology,Humans,Models,Neural Inhibition,Nonlinear Dynamics,Perception,Perception: physiology,Psychological,Stochastic Processes,Time Factors,Visual Perception,Visual Perception: physiology},
  Pmid                     = {11488378},
