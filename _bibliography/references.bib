@Article{Swinnen1998,
  Title                    = {Exploring interlimb constraints during bimanual graphic performance: effects of muscle grouping and direction.},
  Author                   = {Swinnen, S. P. and Jardin, K. and Verschueren, S. and Meulenbroek, R. and Franz, L. and Dounskaia, N. and Walter, C. B.},
