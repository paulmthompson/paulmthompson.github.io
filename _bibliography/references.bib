  Timestamp                = {2015.09.27},
  Url                      = {http://dx.doi.org/10.1126/science.1179867}
}

@Article{Edgerton,
  Title                    = {{Rehabilitative therapies after spinal cord injury.}},
  Author                   = {Edgerton, V Reggie and Kim, Soo J and Ichiyama, Ronaldo M and Gerasimenko, Yuri P and Roy, Roland R},
  Journal                  = {Journal of neurotrauma},
  Number                   = {3-4},
  Pages                    = {560--70},
  Volume                   = {23},

  Abstract                 = {We review some basic and highly relevant concepts in the effort to develop improved rehabilitative interventions for subjects with spinal cord injury (SCI). Interventions that are likely to contribute to improved sensorimotor function include (1) practice of the specific motor task that needs to be improved; and (2) combining the training with one or more interventions--such as pharmacological modulation of the excitability of spinal neural networks, implantation of selected cell types such as olfactory ensheathing glia (OEG), and/or modulation of the excitability of the spinal cord via epidural stimulation. Upon improvement of the neural control of the musculature following SCI, it will always be prudent to maximize the torque output from these activation patterns by assuring that muscle mass is maintained. Therefore, it seems quite feasible that considerable improvement in locomotor performance can be achieved by improved coordination of motor pools, as well as effective recovery of muscle mass, which will assist in the potential generation of normal forces among agonistic and antagonistic muscle groups.},
  Doi                      = {10.1089/neu.2006.23.560},
  ISSN                     = {0897-7151},
  Keywords                 = {Animals,Combined Modality Therapy,Humans,Locomotion,Locomotion: physiology,Neural Pathways,Neural Pathways: physiology,Posture,Posture: physiology,Spinal Cord Injuries,Spinal Cord Injuries: rehabilitation},
  Pmid                     = {16629637},
  Url                      = {http://www.ncbi.nlm.nih.gov/pubmed/16629637}
}

@Article{Edgerton2004,
  Title                    = {{Plasticity of the spinal neural circuitry after injury.}},
  Author                   = {Edgerton, V Reggie and Tillakaratne, Niranjala J K and Bigbee, Allison J and de Leon, Ray D and Roy, Roland R},
  Journal                  = {Annual review of neuroscience},
  Year                     = {2004},

  Month                    = {\#jan\#},
  Pages                    = {145--67},
  Volume                   = {27},

  Abstract                 = {Motor function is severely disrupted following spinal cord injury (SCI). The spinal circuitry, however, exhibits a great degree of automaticity and plasticity after an injury. Automaticity implies that the spinal circuits have some capacity to perform complex motor tasks following the disruption of supraspinal input, and evidence for plasticity suggests that biochemical changes at the cellular level in the spinal cord can be induced in an activity-dependent manner that correlates with sensorimotor recovery. These characteristics should be strongly considered as advantageous in developing therapeutic strategies to assist in the recovery of locomotor function following SCI. Rehabilitative efforts combining locomotor training pharmacological means and/or spinal cord electrical stimulation paradigms will most likely result in more effective methods of recovery than using only one intervention.},
  Doi                      = {10.1146/annurev.neuro.27.070203.144308},
  ISSN                     = {0147-006X},
  Keywords                 = {Animals,Electric Stimulation Therapy,Electric Stimulation Therapy: trends,Gait Disorders, Neurologic,Gait Disorders, Neurologic: physiopathology,Gait Disorders, Neurologic: rehabilitation,Humans,Locomotion,Locomotion: physiology,Neural Pathways,Neural Pathways: physiology,Neural Pathways: physiopathology,Neuronal Plasticity,Neuronal Plasticity: physiology,Physical Fitness,Physical Fitness: physiology,Recovery of Function,Recovery of Function: physiology,Spinal Cord,Spinal Cord Injuries,Spinal Cord Injuries: physiopathology,Spinal Cord Injuries: rehabilitation,Spinal Cord: physiology,Spinal Cord: physiopathology},
  Pmid                     = {15217329},
