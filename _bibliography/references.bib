@Article{Panzeri2001,
  Title                    = {{The role of spike timing in the coding of stimulus location in rat somatosensory cortex.}},
  Author                   = {Panzeri, S and Petersen, R S and Schultz, S R and Lebedev, M and Diamond, M E},
  Journal                  = {Neuron},
