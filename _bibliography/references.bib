}

@Article{Rowekamp2011,
  Title                    = {Analyzing multicomponent receptive fields from neural responses to natural stimuli.},
  Author                   = {Rowekamp, Ryan J. and Sharpee, Tatyana O.},
  Journal                  = {Network},
  Year                     = {2011},
  Number                   = {1-4},
  Pages                    = {45--73},
  Volume                   = {22},

  Abstract                 = {The challenge of building increasingly better models of neural responses to natural stimuli is to accurately estimate the multiple stimulus features that may jointly affect the neural spike probability. The selectivity for combinations of features is thought to be crucial for achieving classical properties of neural responses such as contrast invariance. The joint search for these multiple stimulus features is difficult because estimating spike probability as a multidimensional function of stimulus projections onto candidate relevant dimensions is subject to the curse of dimensionality. An attractive alternative is to search for relevant dimensions sequentially, as in projection pursuit regression. Here we demonstrate using analytic arguments and simulations of model cells that different types of sequential search strategies exhibit systematic biases when used with natural stimuli. Simulations show that joint optimization is feasible for up to three dimensions with current algorithms. When applied to the responses of V1 neurons to natural scenes, models based on three jointly optimized dimensions had better predictive power in a majority of cases compared to dimensions optimized sequentially, with different sequential methods yielding comparable results. Thus, although the curse of dimensionality remains, at least several relevant dimensions can be estimated by joint information maximization.},
  Doi                      = {10.3109/0954898X.2011.566303},
  File                     = {:Rowekamp2011.pdf:PDF},
  Institution              = {Computational Neurobiology Laboratory, The Salk Institute for Biological Studies, La Jolla, CA 92037, USA.},
  Keywords                 = {Action Potentials, physiology; Algorithms; Animals; Computer Simulation; Humans; Models, Neurological; Neurons, physiology; Nonlinear Dynamics},
  Language                 = {eng},
