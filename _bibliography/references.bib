}

@Article{Shoham2005,
  Title                    = {Statistical encoding model for a primary motor cortical brain-machine interface.},
  Author                   = {Shoham, Shy and Paninski, Liam M. and Fellows, Matthew R. and Hatsopoulos, Nicholas G. and Donoghue, John P. and Normann, Richard A.},
