  Keywords                 = {Area 1,S1,S2,area 2,area 3a,area 3b,medial parietal cortex,parietal ventral area,posterior parietal cortex,primary somatosensory cortex},
  Pages                    = {143--169},
  Url                      = {http://www.sciencedirect.com/science/article/pii/B9780123708809003509}
}

@Article{QuianQuiroga2009a,
