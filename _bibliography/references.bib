  Keywords                 = {Action Potentials,Action Potentials: physiology,Animals,Brain,Brain: anatomy \& histology,Brain: physiology,Computer-Assisted,Computer-Assisted: instrumentat,Electrodes,Humans,Implanted,Implanted: trends,Motor Cortex,Motor Cortex: physiology,Movement Disorders,Movement Disorders: physiopathology,Movement Disorders: therapy,Neurons,Neurons: physiology,Prostheses and Implants,Prostheses and Implants: trends,Robotics,Robotics: methods,Robotics: trends,Signal Processing,User-Computer Interface},
  Pmid                     = {19400719},
  Url                      = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2921719\&tool=pmcentrez\&rendertype=abstract}
}

