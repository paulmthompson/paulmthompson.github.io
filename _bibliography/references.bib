  Author                   = {Imfeld, Kilian and Neukom, Simon and Maccione, Alessandro and Bornat, Yannick and Martinoia, Sergio and Farine, Pierre-Andr\'{e} and Koudelka-Hep, Milena and Berdondini, Luca},
  Journal                  = {IEEE transactions on bio-medical engineering},
  Year                     = {2008},
  Number                   = {8},
