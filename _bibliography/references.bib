@Article{Mussa-Ivaldi2010,
  Title                    = {{New Perspectives on the Dialogue between Brains and Machines.}},
  Author                   = {Mussa-Ivaldi, Ferdinando A and Alford, Simon T and Chiappalone, Michela and Fadiga, Luciano and Karniel, Amir and Kositsky, Michael and Maggiolini, Emma and Panzeri, Stefano and Sanguineti, Vittorio and Semprini, Marianna and Vato, Alessandro},
  Journal                  = {Frontiers in neuroscience},
  Year                     = {2010},

  Month                    = {\#jan\#},
  Pages                    = {44},
  Volume                   = {4},

  Abstract                 = {Brain-machine interfaces (BMIs) are mostly investigated as a means to provide paralyzed people with new communication channels with the external world. However, the communication between brain and artificial devices also offers a unique opportunity to study the dynamical properties of neural systems. This review focuses on bidirectional interfaces, which operate in two ways by translating neural signals into input commands for the device and the output of the device into neural stimuli. We discuss how bidirectional BMIs help investigating neural information processing and how neural dynamics may participate in the control of external devices. In this respect, a bidirectional BMI can be regarded as a fancy combination of neural recording and stimulation apparatus, connected via an artificial body. The artificial body can be designed in virtually infinite ways in order to observe different aspects of neural dynamics and to approximate desired control policies.},
  Doi                      = {10.3389/neuro.01.008.2010},
  ISSN                     = {1662-453X},
  Pmid                     = {20589094},
  Url                      = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2920523\&tool=pmcentrez\&rendertype=abstract}
}

@Article{Mussa-Ivaldi2003,
  Title                    = {{Brain-machine interfaces: computational demands and clinical needs meet basic neuroscience.}},
  Author                   = {Mussa-Ivaldi, Ferdinando A and Miller, Lee E},
  Journal                  = {Trends in neurosciences},
  Year                     = {2003},

  Month                    = {\#jun\#},
  Number                   = {6},
  Pages                    = {329--34},
  Volume                   = {26},

  Abstract                 = {As long as 150 years ago, when Fritz and Hitzig demonstrated the electrical excitability of the motor cortex, scientists and fiction writers were considering the possibility of interfacing a machine with the human brain. Modern attempts have been driven by concrete technological and clinical goals. The most advanced of these has brought the perception of sound to thousands of deaf individuals by means of electrodes implanted in the cochlea. Similar attempts are underway to provide images to the visual cortex and to allow the brains of paralyzed patients to re-establish control of the external environment via recording electrodes. This review focuses on two challenges: (1) establishing a 'closed loop' between sensory input and motor output and (2) controlling neural plasticity to achieve the desired behavior of the brain-machine system. Meeting these challenges is the key to extending the impact of the brain-machine interface.},
  Doi                      = {10.1016/S0166-2236(03)00121-8},
  File                     = {:Mussa-Ivaldi, Miller - 2003 - Brain-machine interfaces computational demands and clinical needs meet basic neuroscience.pdf:PDF},
