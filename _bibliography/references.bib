@Article{Pandarinath2015,
  Title                    = {Neural population dynamics in human motor cortex during movements in people with ALS.},
  Author                   = {Pandarinath, Chethan and Gilja, Vikash and Blabe, Christine H. and Nuyujukian, Paul and Sarma, Anish A. and Sorice, Brittany L. and Eskandar, Emad N. and Hochberg, Leigh R. and Henderson, Jaimie M. and Shenoy, Krishna V.},
