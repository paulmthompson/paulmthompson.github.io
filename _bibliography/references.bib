@Article{Patterson2004,
  Title                    = {{A microelectrode/microelectronic hybrid device for brain implantable neuroprosthesis applications.}},
  Author                   = {Patterson, William R and Song, Yoon-Kyu and Bull, Christopher W and Ozden, Ilker and Deangellis, Andrew P and Lay, Christopher and McKay, J Lucas and Nurmikko, Arto V and Donoghue, John D and Connors, Barry W},
  Journal                  = {IEEE transactions on bio-medical engineering},
  Year                     = {2004},

