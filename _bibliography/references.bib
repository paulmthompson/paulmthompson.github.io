}

@Article{Hutzler2006,
  Title                    = {{High-resolution multitransistor array recording of electrical field potentials in cultured brain slices.}},
  Author                   = {Hutzler, M and Lambacher, A and Eversmann, B and Jenkner, M and Thewes, R and Fromherz, P},
  Journal                  = {Journal of neurophysiology},
