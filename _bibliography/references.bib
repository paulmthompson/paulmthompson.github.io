@Article{Lebedev2011,
  Title                    = {{Future developments in brain-machine interface research.}},
  Author                   = {Lebedev, Mikhail A and Tate, Andrew J and Hanson, Timothy L and Li, Zheng and O'Doherty, Joseph E and Winans, Jesse A and Ifft, Peter J and Zhuang, Katie Z and Fitzsimmons, Nathan A and Schwarz, David A and Fuller, Andrew M and An, Je Hi and Nicolelis, Miguel A L},
