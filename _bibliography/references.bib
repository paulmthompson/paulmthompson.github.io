@Article{Stevenson2008,
  Title                    = {Inferring functional connections between neurons.},
  Author                   = {Stevenson, Ian H. and Rebesco, James M. and Miller, Lee E. and K{\"{o}}rding, Konrad P.},
  Journal                  = {Curr Opin Neurobiol},
