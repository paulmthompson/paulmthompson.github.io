@Article{Taylor2002,
  Title                    = {{Direct cortical control of 3D neuroprosthetic devices.}},
  Author                   = {Taylor, Dawn M and Tillery, Stephen I Helms and Schwartz, Andrew B},
  Journal                  = {Science (New York, N.Y.)},
  Year                     = {2002},

  Month                    = {\#jun\#},
  Number                   = {5574},
