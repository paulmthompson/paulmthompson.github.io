  Url                      = {http://dx.doi.org/10.1146/annurev-neuro-062012-170253}
}

@Article{Sharpee2011,
  Title                    = {Hierarchical representations in the auditory cortex.},
  Author                   = {Sharpee, Tatyana O. and Atencio, Craig A. and Schreiner, Christoph E.},
