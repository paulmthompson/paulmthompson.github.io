  Keywords                 = {Adolescent,Adult,Arm,Arm: physiology,Biomechanical Phenomena,Biomechanical Phenomena: physiology,Female,Functional Laterality,Functional Laterality: physiology,Humans,Male,Movement,Movement: physiology,Photic Stimulation,Photic Stimulation: methods,Psychomotor Performance,Psychomotor Performance: physiology,Young Adult},
  Pmid                     = {22114275},
