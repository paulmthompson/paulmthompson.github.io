@Article{Acharya2008,
  Title                    = {{Decoding individuated finger movements using volume-constrained neuronal ensembles in the M1 hand area.}},
  Author                   = {Acharya, Soumyadipta and Tenore, Francesco and Aggarwal, Vikram and Etienne-Cummings, Ralph and Schieber, Marc H and Thakor, Nitish V},
  Journal                  = {IEEE transactions on neural systems and rehabilitation engineering : a publication of the IEEE Engineering in Medicine and Biology Society},
  Year                     = {2008},

  Month                    = {\#mar\#},
  Number                   = {1},
  Pages                    = {15--23},
  Volume                   = {16},

  Abstract                 = {Individuated finger and wrist movements can be decoded using random subpopulations of neurons that are widely distributed in the primary motor (M1) hand area. This work investigates 1) whether it is possible to decode dexterous finger movements using spatially-constrained volumes of neurons as typically recorded from a microelectrode array; and 2) whether decoding accuracy differs due to the configuration or location of the array within the M1 hand area. Single-unit activities were sequentially recorded from task-related neurons in two rhesus monkeys as they performed individuated movements of the fingers and the wrist. Simultaneous neuronal ensembles were simulated by constraining these activities to the recording field dimensions of conventional microelectrode array architectures. Artificial neural network (ANN) based filters were able to decode individuated finger movements with greater than 90\% accuracy for the majority of movement types, using as few as 20 neurons from these ensemble activities. Furthermore, for the large majority of cases there were no significant differences (p < 0.01) in decoding accuracy as a function of the location of the recording volume. The results suggest that a brain-machine interface (BMI) for dexterous control of individuated fingers and the wrist can be implemented using microelectrode arrays placed broadly in the M1 hand area.},
  Doi                      = {10.1109/TNSRE.2007.916269},
  File                     = {:Acharya et al. - 2008 - Decoding individuated finger movements using volume-constrained neuronal ensembles in the M1 hand area.pdf:PDF},
  ISSN                     = {1534-4320},
  Keywords                 = {Algorithms,Animals,Computer Simulation,Conditioning,Efferent Pathways,Efferent Pathways: cytology,Efferent Pathways: physiology,Fingers,Fingers: innervation,Fingers: physiology,Hand,Hand: innervation,Hand: physiology,Macaca mulatta,Male,Microelectrodes,Models,Motor Cortex,Motor Cortex: physiology,Motor Neurons,Motor Neurons: physiology,Movement,Movement: physiology,Nonlinear Dynamics,Operant,Operant: physiology,Prosthesis Design,Statistical,Wrist,Wrist: innervation,Wrist: physiology},
  Pmid                     = {18303801},
  Url                      = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2713669\&tool=pmcentrez\&rendertype=abstract}
}

@Article{Adan2003a,
  Title                    = {{Diagnosis and treatment of sick sinus syndrome.}},
  Author                   = {Adan, Victor and Crown, Loren A},
  Journal                  = {American family physician},
  Year                     = {2003},

  Month                    = {\#apr\#},
  Number                   = {8},
  Pages                    = {1725--32},
  Volume                   = {67},

  Abstract                 = {Sick sinus syndrome comprises a variety of conditions involving sinus node dysfunction and commonly affects elderly persons. While the syndrome can have many causes, it usually is idiopathic. Patients may experience syncope, pre-syncope, palpitations, or dizziness; however, they often are asymptomatic or have subtle or nonspecific symptoms. Sick sinus syndrome has multiple manifestations on electrocardiogram, including sinus bradycardia, sinus arrest, sinoatrial block, and alternating patterns of bradycardia and tachycardia (bradycardia-tachycardia syndrome). Diagnosis of sick sinus syndrome can be difficult because of its nonspecific symptoms and elusive findings on electrocardiogram or Holter monitor. The mainstay of treatment is atrial or dual-chamber pacemaker placement, which generally provides effective relief of symptoms and lowers the incidence of atrial fibrillation, thromboembolic events, heart failure, and mortality, compared with ventricular pacemakers.},
  File                     = {:Adan, Crown - 2003 - Diagnosis and treatment of sick sinus syndrome.pdf:PDF},
  ISSN                     = {0002-838X},
  Keywords                 = {Aged,Artificial,Electrocardiography,Female,Humans,Male,Pacemaker,Prognosis,Sick Sinus Syndrome,Sick Sinus Syndrome: diagnosis,Sick Sinus Syndrome: physiopathology,Sick Sinus Syndrome: therapy},
  Pmid                     = {12725451},
  Url                      = {http://www.ncbi.nlm.nih.gov/pubmed/12725451}
}

