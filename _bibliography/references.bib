@Article{Amirikian2003,
  Title                    = {{Modular organization of directionally tuned cells in the motor cortex: is there a short-range order?}},
  Author                   = {Amirikian, Bagrat and Georgopoulos, Apostolos P},
  Journal                  = {Proceedings of the National Academy of Sciences of the United States of America},
  Year                     = {2003},

  Month                    = {\#oct\#},
  Number                   = {21},
  Pages                    = {12474--9},
  Volume                   = {100},

  Abstract                 = {We investigated the presence of short-range order (<600 microm) in the directional properties of neurons in the motor cortex of the monkey. For that purpose, we developed a quantitative method for the detection of functional cortical modules and used it to examine such potential modules formed by directionally tuned cells. In the functional domain, we labeled each cell by its preferred direction (PD) vector in 3D movement space; in the spatial domain, we used the position of the tip of the recording microelectrode as the cell's coordinate. The images produced by this method represented two orthogonal dimensions in the cortex; one was parallel ("horizontal") and the other perpendicular ("vertical") to the cortical layers. The distribution of directionally tuned cells in these dimensions was nonuniform and highly structured. Specifically, cells with similar PDs tended to segregate into vertically oriented minicolumns 50-100 microm wide and at least 500 microm high. Such minicolumns aggregated across the horizontal dimension in a secondary structure of higher order. In this structure, minicolumns with similar PDs were approximately 200 microm apart and were interleaved with minicolumns representing nearly orthogonal PDs; in addition, nonoverlapping columns representing nearly opposite PDs were approximately 350 microm apart.},
  Doi                      = {10.1073/pnas.2037719100},
  File                     = {:Amirikian, Georgopoulos - 2003 - Modular organization of directionally tuned cells in the motor cortex is there a short-range order.pdf:PDF},
  ISSN                     = {0027-8424},
  Keywords                 = {Animals,Cell Count,Electrophysiology,Haplorhini,Haplorhini: anatomy \& histology,Haplorhini: physiology,Microelectrodes,Models, Neurological,Monte Carlo Method,Motor Cortex,Motor Cortex: cytology,Motor Cortex: physiology},
  Pmid                     = {14523235},
  Url                      = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=218782\&tool=pmcentrez\&rendertype=abstract}
}

@Article{Andersen2009,
  Title                    = {Intention, action planning, and decision making in parietal-frontal circuits.},
