@Article{Marcos2013,
  Title                    = {{Neural variability in premotor cortex is modulated by trial history and predicts behavioral performance.}},
  Author                   = {Marcos, Encarni and Pani, Pierpaolo and Brunamonti, Emiliano and Deco, Gustavo and Ferraina, Stefano and Verschure, Paul},
